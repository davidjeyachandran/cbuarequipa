{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "precommit": "npm run lint && git add .",
    "format": "prettier '**/*.{js,ts,jsx,tsx}' --write",
    "develop": "env-cmd -f .env.development gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "lint": "eslint --ext .js,.js .",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && gatsby clean && npm install && npm start",
    "start": "npm run develop"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.0",
    "classnames": "2.2.6",
    "core-js": "latest",
    "gatsby": "latest",
    "gatsby-image": "^2.2.40",
    "gatsby-plugin-catch-links": "^2.1.25",
    "gatsby-plugin-feed": "^2.3.27",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-lodash": "^3.1.20",
    "gatsby-plugin-manifest": "^2.0.22",
    "gatsby-plugin-material-ui": "1.2.5",
    "gatsby-plugin-nprogress": "^2.1.19",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.8",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-twitter": "^2.1.19",
    "gatsby-remark-autolink-headers": "^2.1.24",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-prismjs": "^3.3.31",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.32",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.50",
    "history": "^4.7.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "nouislider": "^13.1.1",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-datetime": "^2.16.3",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-icon": "^1.0.0",
    "react-icons": "^3.5.0",
    "react-image-gallery": "^1.0.6",
    "react-jss": "^10.0.4",
    "react-router-dom": "^4.3.1",
    "react-share": "^4.0.1",
    "react-slick": "^0.23.2",
    "react-swipeable-views": "^0.13.1",
    "typeface-roboto": "0.0.54",
    "typeface-roboto-slab": "0.0.72",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.2",
    "gh-pages": "^2.2.0",
    "prettier": "^1.17.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^19.0.0",
    "write-good": "^1.0.1",
    "env-cmd": "^10.1.0",
    "eslint-plugin-react-hooks": "^2.3.0"
  },
  "keywords": [
    "gatsby"
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
